{
  "parser": "babel-eslint",
  "plugins": ["react", "prettier", "jsx-a11y", "import"],
  "extends": ["react-app", "prettier", "prettier/react"],
  "globals": {
    "expect": true,
    "describe": true,
    "it": true,
    "fixture": true,
    "test": true,
    "jest": true,
    "document": true,
    "window": true,
    "fetch": true,
    "navigator": true
  },
  "rules": {
    "no-console": "off",
    "space-before-function-paren": 0,
    "capitalized-comments": "off",
    "quotes": 0,
    "operator-linebreak": 0,
    "require-await": "off",
    "no-unused-vars": 0,
    "valid-jsdoc": "off"
  }
}
